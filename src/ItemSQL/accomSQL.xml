<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <!--  숙소 지역  -->
  <mapper namespace="home">
  
    <!--  서귀포에 있는 숙소 찾기 -->
  <select id="jejusi" resultType="homeDTO">   
  	select * from home where home_local='jejusi'
  </select>
  
  
  <!--  서귀포에 있는 숙소 찾기 -->
  <select id="seoquiposi" resultType="homeDTO">   
  	select * from home where home_local='seoquiposi'
  </select>
  
   <!--  중문에 있는 숙소 찾기 -->
   <select id="jungmun" resultType="homeDTO">  
  	select * from home where home_local='jungmun'
  </select>
  
   <!--  제주공항에 있는 숙소 찾기 -->
    <select id="jejuairport" resultType="homeDTO"> 
  	select * from home where home_local='jejuairport'
  </select>
  
     <!--  애월/한림/협재에 있는 숙소 찾기 -->
    <select id="aweol" resultType="homeDTO"> 
  	select * from home where home_local='aweol'
  </select>
  
     <!--  표선/상산에 있는 숙소 찾기 -->
   <select id="pyoseon" resultType="homeDTO"> 
  	select * from home where home_local='pyoseon'
  </select>
  
  <!-- "hamduk">함덕/김녕/세화 -->
     <select id="hamduk" resultType="homeDTO"> 
  	select * from home where home_local='hamduk'
  </select>
  
  </mapper>
